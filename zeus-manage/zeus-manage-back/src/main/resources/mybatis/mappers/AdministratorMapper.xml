<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.taotao.manage.mapper.AdministratorMapper">

	<select id="queryAdminByUserNameAndPassword" parameterType="com.zeus.manage.bean.Admin" resultType="com.zeus.manage.bean.Admin">
		SELECT * FROM tb_admin WHERE username = #{username} and password = #{password}
	</select>
	
</mapper>
